expit <- function(x)
exp(x) / (1 + exp(x))
negLogLike <- function(theta, v=1, n=5)
- dbinom(v, size=n, prob=expit(theta), log=TRUE)
fit <- optim(par=.5, fn=negLogLike, method='BFGS', hessian=TRUE)
fit$par
theta.mle <- fit$par
theta.se <- sqrt(1 / fit$hessian)
zcrit <- qnorm(.975)
c(- zcrit, zcrit) * theta.se + theta.mle
expit(c(- zcrit, zcrit) * theta.se + theta.mle)
